                                                        <a class="add" data-bs-toggle="modal" data-bs-target="#purchaseModal@(voucher.Vouchers_ID)"><i class="fi-rs-shopping-cart mr-5"></i>Redeem</a>
                                                    </div>

                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                }



                            }

                        }
                        else
                        {
