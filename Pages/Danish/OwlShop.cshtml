                <div class="col-xl-3 col-lg-4 col-md-6">
                    @{ if (Model.allVouchers != null)
                        {
                            @foreach (var voucher in Model.allVouchers)
                            {
                                if (voucher.Super_Deal == true)
                                {
                                    <div class="modal fade" id="purchaseModal@(voucher.Vouchers_ID)" tabindex="-1" aria-labelledby="#purchaseModal@(voucher.Vouchers_ID)" aria-hidden="true">
                                        <div class="modal-dialog">
                                            <div class="modal-content">
                                                <div class="modal-header">
                                                    <h5 class="modal-title" id="exampleModalLabel">Modal title</h5>
                                                    <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Close"></button>
                                                </div>
                                                <div class="modal-body">
                                                    Are you sure you want to buy this?
                                                </div>
                                                <div class="modal-footer">
                                                    <button type="button" class="btn btn-secondary" data-bs-dismiss="modal">Close</button>
