@page
@model OwlOProjectA.Pages.OwleeRegisterModel
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
}

<script type="text/javascript">
    function scanDoc() {
        $.get("/home/scanDoc", function (data) {
            $("p").html(data);
        });
    }



</script>
<!-- Favicon -->
<link rel="shortcut icon" type="image/x-icon" href="~/nest_assets/imgs/theme/favicon.svg" />
<!-- Template CSS -->
<link rel="stylesheet" href="~/nest_assets/css/main.css?v=4.0" />
    <main class="main pages">
        <form method="post">
            

            <div class="page-header breadcrumb-wrap">
                <div class="container">
                    <div class="breadcrumb">
                        <a href="index.html" rel="nofollow"><i class="fi-rs-home mr-5"></i>Home</a>
                        <span></span> Pages <span></span> My Account
                    </div>
                </div>
            </div>
            <div class="page-content pt-150 pb-150">
                <div class="container">
                    <div class="row">
                        <div class="col-xl-8 col-lg-10 col-md-12 m-auto">
                            <div class="row">
                                <div class="col-lg-6 col-md-8">
                                    <div class="login_wrap widget-taber-content background-white">
                                        <div class="padding_eight_all bg-white">
                                            <div class="heading_s1">
                                                <h1 class="mb-5">Create an Account</h1>
                                                <p class="mb-30">Already have an account? <a href="page-login.html">Login</a></p>
                                            </div>
                                            <form method="post">
                                                <div class="form-group">
                                                    <input asp-for="MyOwlee.Email" placeholder="Email" />
                                                    <span asp-validation-for="MyOwlee.Email" class="text-danger"></span>
                                                </div>
                                                <div class="form-group">
                                                    <input asp-for="MyOwlee.NRIC" placeholder="NRIC" />
                                                    <span asp-validation-for="MyOwlee.NRIC" class="text-danger"></span>
                                                </div>
                                                <div class="form-group">
                                                    <input asp-for="MyOwlee.Name" placeholder="Name" />
                                                    <span asp-validation-for="MyOwlee.Name" class="text-danger"></span>
                                                </div>
                                                <div class="form-group">
                                                    <input required="" type="password" name="MyOwlee.Password" placeholder="Password" />
                                                    <span asp-validation-for="MyOwlee.Password" class="text-danger"></span>
                                                </div>
                                                <div class="form-group">
                                                    <input required="" type="password" name="MyOwlee.Password" placeholder="Confirm password" />
                                                    <span asp-validation-for="MyOwlee.Password" class="text-danger"></span>
                                                </div>
                                                <div class="form-group">
                                                    <input asp-for="MyOwlee.Bio" placeholder="Bio" />
                                                    <span asp-validation-for="MyOwlee.Bio" class="text-danger"></span>
                                                </div>

                                                <div class="form-group">
                                                    <label class="form-label">Citizenship</label>
                                                    @Html.DropDownList("MyOwlee.Citizenship", new SelectList(Enum.GetValues(typeof(Models.Citizenship))))
                                                    <span asp-validation-for="MyOwlee.Citizenship" class="text-danger"></span>
                                                </div>

                                                <!-- For captcha -->
                                                <!-- <div class="login_footer form-group">
                                            <div class="chek-form">
                                                <input type="text" required="" name="email" placeholder="Security code *" />
                                            </div>
                                            <span class="security-code">
                                                <b class="text-new">8</b>
                                                <b class="text-hot">6</b>
                                                <b class="text-sale">7</b>
                                                <b class="text-best">5</b>
                                            </span>
                                        </div> -->
                                                <!-- radio button -->
                                                <div class="payment_option mb-50">
                                                    <label class="control-label">Gender</label>
                                                    <div class="custome-radio">

                                                        <input class="form-check-input" required="" type="radio" name="MyOwlee.Gender" value="M" id="male" checked="" />
                                                        <label class="form-check-label" for="male">Male</label>
                                                    </div>
                                                    <div class="custome-radio">

                                                        <input class="form-check-input" required="" type="radio" name="MyOwlee.Gender" value="F" id="female" checked="" />
                                                        <label class="form-check-label" for="female">Female</label>
                                                    </div>
                                                    <span asp-validation-for="MyOwlee.Gender" class="text-danger"></span>
                                                </div>


                                                <!-- <div class="login_footer form-group mb-50">
                                            <div class="chek-form">
                                                <div class="custome-checkbox">
                                                    <input class="form-check-input" type="checkbox" name="checkbox" id="exampleCheckbox12" value="" />
                                                    <label class="form-check-label" for="exampleCheckbox12"><span>I agree to terms &amp; Policy.</span></label>
                                                </div>
                                            </div>
                                            <a href="page-privacy-policy.html"><i class="fi-rs-book-alt mr-5 text-muted"></i>Lean more</a>
                                        </div> -->

                                                <div class="form-group mb-30">
                                                    <button type="submit" id="create" class="btn btn-fill-out btn-block hover-up font-weight-bold" name="login">Submit &amp; Register</button>
                                                </div>
                                                <p class="font-xs text-muted"><strong>Note:</strong>Your personal data will be used to support your experience throughout this website, to manage access to your account, and for other purposes described in our privacy policy</p>
                                            </form>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-lg-6 pr-30 d-none d-lg-block">
                                    <div class="card-login mt-115">
                                        <button type="button" name="button" onclick="scanDoc()" class="social-login facebook-login">
                                            <img src="~/nest_assets/imgs/theme/icons/logo-facebook.svg" alt="" />
                                            <span>Scan Document</span>
                                        </button>
                                        <a href="#" class="social-login google-login">
                                            <img src="~/nest_assets/imgs/theme/icons/logo-google.svg" alt="" />
                                            <span>Continue with Google</span>
                                        </a>
                                        <a href="#" class="social-login apple-login">
                                            <img src="~/nest_assets/imgs/theme/icons/logo-apple.svg" alt="" />
                                            <span>Continue with SingPass</span>
                                        </a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>

        </form>
    </main>





    @section Scripts {
        @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    }
